<script type="text/javascript">
	var map;
  var startLat = 37.5374;
  var startLng = 127.004;
	function init() {
    // 지도 출력
		map = new daum.maps.Map(document.getElementById('map'), {
			center: new daum.maps.LatLng(startLat, startLng),
			level: 3
		});
    
    // 마커 출력
		var marker = new daum.maps.Marker({
			position: new daum.maps.LatLng(startLat, startLng)
		});
		marker.setMap(map);

	}

  $(function() {
    init();
    //$(".datepicker").datepicker();
