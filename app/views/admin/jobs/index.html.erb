 <div class="row-fluid">
    <!-- block -->
    <div class="block">
        <div class="navbar navbar-inner block-header">
            <div class="muted pull-left">멘토링 목록</div>
        </div>
        <div class="block-content collapse in">
            <div class="span12">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>멘토</th>
                    <th>종류</th>
                    <th>시작일</th>
                    <th>교육시간</th>
                    <th>난이도</th>
                    <th>필요인원</th>
                    <th>일당</th>
                    <th>관리</th>
                  </tr>
                </thead>
                <tbody>
                  <% @jobs.each do |job| %>
                  <tr class="">
                    <td><%= job.id %></td>
                    <td class="center">
                      <% unless job.mentor.nil? %>
                        <%= link_to job.mentor.email, admin_user_path(job.mentor) %>
                      <% else %>
                        테스트용 데이터
                      <% end %>
                    </td>
                    <td class="center"><%= job.category %></td>
                    <% unless job.time.nil? %>
                      <td class="center"><%= job.time["start"]%></td>
                    <% else %>
                      <td class="center">테스트용 데이터</td>
                    <% end %>
                    <td class="center">12시간</td>
                    <td class="center">상중</td>
                    <td class="center">5명</td>
                    <td class="center">60,000원</td>
                    <td class="center">
                      <a href="#">보기</a> 
                      <a href="#">수정</a> 
                      <a href="#">삭제</a> 
                      <a href="#">멘티</a>
                    </td>
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
        </div>
    </div>
    <!-- /block -->
</div>