<div class="row-fluid">
  <!-- block -->
  <div class="block">
    <div class="navbar navbar-inner block-header">
        <div class="muted pull-left">Q&A 전체 목록</div>
    </div>
    <div class="block-content collapse in">
      <div class="span12">
          <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
          <thead>
            <tr>
              <th>#</th>
              <th>제목</th>
              <th>등록일</th>
              <th>관리</th>
            </tr>
          </thead>
          <tbody>
            <% @questions.each do |question| %>
            <tr class="">
              <td><%= question.id %></td>
              <td><%= question.title %></td>
              <td class="center"><%= question.created_at %></td>
              <td class="center">
                <%= link_to "상세정보", admin_question_path(question) %>
                <%= link_to "수정", edit_admin_question_path(question) %>
                <%= link_to "삭제", admin_question_path(question), method: :delete, confirm: "삭제 OK?" %>
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <%= link_to "테스트 질문 등록", new_admin_question_path, class: "btn btn-primary"%>
    </div>
  </div>
  <!-- /block -->
</div>